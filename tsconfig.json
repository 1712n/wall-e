{
	"compilerOptions": {
		"strict": true,
		"module": "esnext",
		"target": "esnext",
		"lib": ["esnext"],
		"moduleResolution": "bundler",
		"types": [
			"@cloudflare/workers-types/experimental",
		],
		"noEmit": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": false
	}
}
